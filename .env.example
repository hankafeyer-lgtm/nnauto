# Copy to `.env` for local use. Do not commit real secrets.
NODE_ENV=production
PORT=5000

# Database
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB?sslmode=require
# Prefer Neon "pooled" endpoint for long-running servers (recommended).
DATABASE_URL_POOLED=
PRODUCTION_DATABASE_URL=
PGDATABASE=
PGHOST=
PGPORT=5432
PGUSER=
PGPASSWORD=

# Postgres SSL / pooling reliability knobs
# - By default SSL verification is ON for Neon (recommended).
PGSSL_REQUIRE=true
PGSSL_REJECT_UNAUTHORIZED=true
PGPOOL_MAX=5
PGPOOL_IDLE_TIMEOUT_MS=30000
PGPOOL_CONN_TIMEOUT_MS=10000

# Sessions / JWT
SESSION_SECRET=change_me
JWT_SECRET=change_me
ENABLE_SESSION_HEADER_FALLBACK=false

# MailerSend
MAILERSEND_FROM_EMAIL=info@example.com
MAILERSEND_API_KEY=

# Stripe
STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Cebia Autotracer API (paid per request)
CEBIA_ENABLED=false
CEBIA_CLIENT_ID=
CEBIA_CLIENT_SECRET=
# Optional (only if your account requires password grant)
CEBIA_USERNAME=
CEBIA_PASSWORD=
CEBIA_API_BASE_URL=https://app.cebia.com/api/Autotracer
CEBIA_TOKEN_URL=https://www.cebianet.cz/pub/oauth/token
# Your retail price to customer in haléře (e.g. 19900 = 199 CZK)
CEBIA_REPORT_PRICE_CENTS=
CEBIA_STRIPE_PAYMENT_LINK_URL=
CEBIA_PAYMENTS_FROZEN=false
CEBIA_AUTO_REQUEST_ON_PAID=true
CEBIA_EMAIL_ON_READY=false

# Cloudflare Turnstile
TURNSTILE_SITE_KEY=
TURNSTILE_SECRET_KEY=
VITE_TURNSTILE_SITE_KEY=

# Cloudflare R2
CLOUDFLARE_R2_ACCOUNT_ID=
CLOUDFLARE_R2_BUCKET_NAME=
CLOUDFLARE_R2_ACCESS_KEY_ID=
CLOUDFLARE_R2_SECRET_ACCESS_KEY=

# App
BASE_URL=https://example.com
DEFAULT_OBJECT_STORAGE_BUCKET_ID=
PUBLIC_OBJECT_SEARCH_PATHS=
PRIVATE_OBJECT_DIR=

